var quit = false;
$(document).ready(function(){
    if($(window).width() < 650){
        $('#addRelated').hide();
    }
    if($(window).width() <= 1024){
        $('#mobileRelated').show();
        $('#showRelated').hide();
    }
    if($(window).width() >= 650) {
        $(window).scroll(function(event){
            var scrollTop = $(window).scrollTop();
            var st = $(this).scrollTop();
            var commentStart = $('#comment-info-wrapper').offset().top;
            var end = (commentStart - scrollTop);
            var $related = $('#addRelated');
            var width = $(window).width();

            if (st > lastScrollTop){
                // downscroll code
                $related.hide();

            } else {
                // upscroll code
                if(scrollTop > 200 && scrollTop < end){
                    $related.slideDown('slow');
                }
            }
            lastScrollTop = st;

            var $showRelated = $('#showRelated');

            if( (st+700 >= commentStart) && (quit == false) && width > 1024){
                $showRelated.find('li:first').show();
                $showRelated.stop().animate({bottom: '0px'}, 1000);
            } else {
                $showRelated.stop().animate({bottom: '-250px'}, 500);
            }
        });
    }
});



