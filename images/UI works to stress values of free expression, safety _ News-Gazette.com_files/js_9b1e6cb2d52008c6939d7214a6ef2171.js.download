      //we only show a messge (for now) with status code 3 or 5

      switch (pcmres.status)
      {
        case 3: //first limit reached
          var src = "http://" + pcmcfg.hn + "/modal/message1?s=" + window.location.host + "&r=" + pcmcfg.ur;
          break;
        case 5: //hard limit reached
          var src = "http://" + pcmcfg.hn + "/modal/message2?s=" + window.location.host + "&r=" +pcmcfg.ur;
          break;
