$(document).ready(function(){
	$(".term-extra-more").css('display', 'block');
	$(".term-extra").hide();
});

function termControl(nid, vid) {
	if ($("#vocab-" + nid + "-" + vid + "-wrapper .term-extra").css("display") == 'none') {
		$("#vocab-" + nid + "-" + vid + "-wrapper .term-extra").show();
		termShowHide(nid, vid);
	} else {
		$("#vocab-" + nid + "-" + vid + "-wrapper .term-extra").hide();
		termShowHide(nid, vid);
	}
}

function termShowHide(nid, vid) {
	var html = $("#vocab-" + nid + "-" + vid + "-wrapper .term-extra-more").html();
	if (html.search("Hide") != -1) {
		html = html.replace('Hide', 'More');
		html = html.replace('up', 'down');
	} else {
		html = html.replace('More', 'Hide');
		html = html.replace('down', 'up');
	}
	$("#vocab-" + nid + "-" + vid + "-wrapper .term-extra-more").html(html);
};
