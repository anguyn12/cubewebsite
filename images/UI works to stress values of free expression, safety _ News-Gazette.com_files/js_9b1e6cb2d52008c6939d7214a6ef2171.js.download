$(document).ready(function() {

    // submit the login form and get response
    $('#login-form').live('submit', function(e){

        var cookieVal = $.cookie('_pcmcn');
        $('#cookie-value').val(cookieVal);

        e.preventDefault();

        var form_data = $('#login-form').serialize();

        $.ajax({
            type: "GET",
            url: "https://subscriptions.news-gazette.com/modal/login/js",
            dataType: "jsonp",
            data: form_data,
            success: function (data) {

                if(data._pcmss){
                    $.cookie('_pcmss', 1, {
                        path: '/',
                        expires: 31,
                        domain: '.news-gazette.com'
                    });
                }

                if(data.error){
                    $('#login-error').fadeOut();
                    $('.panel-login').fadeIn().prepend('<div class="alert alert-danger" id="login-error">' + data.msg + '</div>');
                } else {
                    $('#login-form').remove();
                    var panel = $('.panel-body');
                    panel.prepend('<div class="alert alert-success">' + data.msg + '</div>');

                    $('#login-error').remove();
                    $('.forgot-password').remove();

                    panel.append('<h3>Subscription</h3>');
                    $.each(data.subs, function(key, value){
                        var d = new Date(value.exp_date);

                        if(value.active){
                            panel.append('<div class="well subscription-info active-sub"><span class="strong">' + value.product_name + '</span> subscription ' +
                                '<a class="btn btn-reactivate" href="https://subscriptions.news-gazette.com/login">Manage</a>' +
                                '</div>');
                        } else {
                            panel.append('<div class="well subscription-info inactive-sub"><span class="strong">' + value.product_name + '</span> subscription ' +
                                '<a class="btn btn-manage" href="https://subscriptions.news-gazette.com/user/profile/' + value.owner_id + '/sub/' + value.id + '/reactivate">Reactivate</a>' +
                                '</div>');
                        }
                    });
                    panel.append('<button class="btn btn-continue" onclick="closeAuth()">Continue</button>');
                    // setTimeout(closeAuth, 1000);
                }
            }
        });
        return false;
    });

    //submit the password reset form and get response
    $('#forgot-modal').live('submit', function(e){
        e.preventDefault();
        var form_data = $('#forgot-modal').serialize();

        $.ajax({
            type: "POST",
            url: "https://subscriptions.news-gazette.com/modal/password",
            data: form_data,
            success: function (data) {
                var response = JSON.parse(data);
                if(response["error"]){
                    $('#forgot-modal').fadeOut();
                    $('.panel-login').fadeIn().prepend('<div class="alert alert-danger" id="login-error">' + response["msg"] + '</div>');
                } else {
                    $('#forgot-modal').remove();
                    $('.panel-body').prepend('<div class="alert alert-success">' + response["msg"] + '</div>');
                    setTimeout(closeAuth, 1000);
                }
            }
        });
        return false;
    });
});

