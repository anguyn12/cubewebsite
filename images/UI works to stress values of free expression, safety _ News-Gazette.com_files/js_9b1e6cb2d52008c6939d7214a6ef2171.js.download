    function showDisplay(pcmres)
    {
        console.log('pcmres: ' +pcmres);
        switch(pcmres.display){
            case 'overlay':
                return showOverlayDisplay(pcmres.html, pcmres.image);
                break;
            case 'modal':
                return showModalDisplay(pcmres.html);
                break;
            case 'slide':
                return showSlideDisplay(pcmres.html);
                break;
            default:
                return false;
        }
    }

    function showOverlayDisplay(html, img)
    {
        document.documentElement.style.overflow = 'hidden';  // firefox, chrome
        document.body.scroll = "no"; // ie only

        display = $('#displayOverlay');
        bg = 'http://subscriptions.news-gazette.com/images/meter/' + img;

