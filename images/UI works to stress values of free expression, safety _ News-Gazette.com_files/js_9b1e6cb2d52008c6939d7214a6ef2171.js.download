          break;
        case 2:
          pcmres.status = 5;
          break;
      }

      if (pcmcfg.count_only == 0) { //don't show messages if we're in counter-only mode

        switch (pcmres.display) {
          case 0: //error in meter application
              return false;
              break;
          case 1: //new visit, redirect to central domain so that cookie can be set -- NOT IMPLEMENTED
