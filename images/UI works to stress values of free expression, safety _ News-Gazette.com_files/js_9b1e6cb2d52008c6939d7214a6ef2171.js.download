// After the page loads, see what the value of all 'Select all' checkboxes should be
$(document).ready(function() {
  if ('.taxonomy-super-select-all') {
    $(".taxonomy-super-selector-all").each(function(){
      var tid = $(this).attr('id').match(/\d+$/);
      var selectAllId = '#' + $(this).attr('id');
      taxonomyTermSelections(tid, selectAllId);
    });
  }
});

// Given a 'Select all' tid and element id, loop through terms to see which are checked.
function taxonomyTermSelections(tid, selectAllId) {
  var unchecked = 0;
  $(".taxonomy-super-select-all-" + tid).each(function(){
    if (!$(this).attr('checked')) {
      unchecked = 1;
    }
  });
  if (!unchecked) {
    $(selectAllId).attr('checked', true);
  } else {
    $(selectAllId).attr('checked', false);
  }
}

// When a term checkbox is (un)checked, check what the value of 'Select all' should be
function clickSelectTaxonomyCheck(vid, tid) {
  var selectAllId = '#edit-taxonomy-' + vid + '-taxonomy-super-select-vid-' + vid + '-' + tid;
  taxonomyTermSelections(tid, selectAllId);
}

// Check or uncheck all terms when a 'Select all' checkbox is (un)checked
function checkAllTaxonomy(className) {
  var tid = className.match(/\d+$/);
  if ($('.' + className).attr('checked') == true) {
    $('.taxonomy-super-select-all-' + tid).attr('checked', true);
  } else {
    $('.taxonomy-super-select-all-' + tid).attr('checked', false);
  }
};
$(document).ready(function(){
  $("body").click(function(){
    if ($(".search-options-arrow").length ) {
      $(".search-options-dropdown").hide();
      $(".search-options-arrow").attr("src", $(".search-options-arrow").attr("src").replace('up', 'down'));
    }
  });
  $(".search-options-arrow").click (function (e) {
    e.stopPropagation();
    searchDropdown();
    return false;
  });
  $(".search-options-dropdown").click (function (e) {
    e.stopPropagation();
  });
  $(".search-theme-form-text-input").click (function (e) {
    e.stopPropagation();
  });
  $(".search-theme-form-text-input").focus (function () {
    searchDropdownShow();
  });
});

function searchDropdown() {
  if ($(".search-options-dropdown").css("display") == 'none') {
    searchDropdownShow();
  } else {
    searchDropdownHide();
  }
}

function searchDropdownHide() {
  $(".search-options-dropdown").hide();
  $(".search-options-arrow").attr("src", $(".search-options-arrow").attr("src").replace('up', 'down'));
}

function searchDropdownShow() {
  $(".search-options-dropdown").css("display", "inline");
  $(".search-options-arrow").attr("src", $(".search-options-arrow").attr("src").replace('down', 'up'));
};
