          return false;
          break;
        default:
          return false;
      }

//      if(pcmcfg.device == 'phone') {
      if($(window).width() < 750) {
        $.modal('<iframe src="' + src + '" height="430" width="320" style="padding:0px;" class="noscroll">', {
          closeHTML:"<a class='modalCloseImg' title='Close'></a>",
          containerCss:{
            //backgroundColor:"url('trans.png')", //ie8 does not like backgroundColor: "none";
            borderColor:"#fff",
            height:440,
            padding:0,
            width:320

          },
          position: [80,10],
          //maxHeight:440,
         // maxWidth:330,
          modal:true,
          overlayClose:false,
          opacity:85,
          overlayCss:{backgroundColor:"#000"},
          onClose:function() {
            if (pcmres.status==5) {
              window.location.replace('/'); //relative path takes them to the home page on the current domain
            }
            else {
              $.modal.close();
            }
          }
        });
      } // end modal for phones
      else {
        $.modal('<iframe src="' + src + '" height="370" width="740" class="noscroll">', {
          closeHTML:"<a class='modalCloseImg' title='Close'></a>",
          containerCss:{
            //backgroundColor:"url('trans.png')", //ie8 does not like backgroundColor: "none";
            borderColor:"#fff",
            height:380,
            padding:0,
            width:750
          },
          modal:true,
          overlayClose:false,
          opacity:85,
          overlayCss:{backgroundColor:"#000"},
          onClose:function() {
            if (pcmres.status==5) {
              window.location.replace('/'); //relative path takes them to the home page on the current domain
            }
            else {
              $.modal.close();
            }
          }
        });
      } // end modal for desktop/tablet
    }


  };


  /* ///////////////////////////////////////// */
  var pcmcfg = null;
  if (typeof Drupal.settings.pc_meter !== 'undefined')
    pcmcfg = $.extend({}, Drupal.settings.pc_meter.params);

  $.cookie('_pcmcck', 'tngi', { path: '/' });
    if ($.cookie('_pcmcck') == 'tngi') {
       pcmcfg.ce = 1;
    } else {//cookies are turned off
       pcmcfg.ce = 0;
    }

  function pcm_do() {

    $.getJSON('http://' + pcmcfg.hn + pcmcfg.ep + '?callback=?', {
      ce: pcmcfg.ce,
      si: window.location.host,
      cv: ($.cookie(pcmcfg.cn)) ? $.cookie(pcmcfg.cn) : '',
      pc: ($.cookie('_pcmcnt')) ? $.cookie('_pcmcnt') : '',
      ni: pcmcfg.ni,
      ct: pcmcfg.ct,
      ap: pcmcfg.ap,
      cr: pcmcfg.cr,
      ch: pcmcfg.ch,
      sl: pcmcfg.sl,
      rf: document.referrer,
      ur: pcmcfg.ur,
      sec_t: pcmcfg.sec_t,
      top_t: pcmcfg.top_t,
      pr: pcmcfg.pr
